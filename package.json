{
  "name": "formilydemo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "analyzer": "cross-env NODE_ENV=analyzer webpack -c ./config/ana.config.js",
    "start": "npm run dev",
    "prd": "cross-env NODE_ENV=production webpack -c ./config/prd.config.js",
    "dev": "cross-env NODE_ENV=development webpack serve --color -c ./config/dev.config.js",
    "serve": "serve dist -C -l 9900",
    "pretty:code": "prettier --cache --write \"{src,mock}/**/*.{js,ts,tsx,css,less,md,json}\"",
    "lint:ts": "tsc --noEmit && eslint '*/**/*.{js,jsx,ts,tsx}' --quiet --fix --cache",
    "lint:css": "stylelint src/**/*.css --fix",
    "lint:less": "stylelint --color --fix src/**/*.{html,css,less} --custom-syntax postcss-less --cache",
    "lint:scss": "stylelint --color --fix src/**/*.{html,css,scss} --custom-syntax postcss-scss --cache",
    "compress": "tinyit ./src ./src -key=HVm5yFJFTBDVsSfmVX083ZQShzLqZvM3 -filter=true",
    "test": "jest --coverage --cache  --color",
    "precommit": "npm run lint:less && npm run lint:ts && npm run pretty:code",
    "clear": "rimraf 'packages/*/{lib,node_modules}' && rimraf node_modules",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "babel-loader": "^9.1.0",
    "babel-plugin-import": "^1.13.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.2",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "dayjs": "^1.11.6",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.7.0",
    "postcss": "^8.4.19",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.7.1",
    "react-refresh": "^0.14.0",
    "react-router-dom": "^6.4.3",
    "style-loader": "^3.3.1",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-webpack-plugin": "^3.3.0",
    "terser-webpack-plugin": "^5.3.6",
    "thread-loader": "^3.0.4",
    "typescript": "4.8.3",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^4.10.0",
