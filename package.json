  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "ahooks": "^3.7.2",
    "antd": "5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css,less}": [
      "stylelint --fix --custom-syntax postcss-less --cache",
      "prettier --write"
    ],
    "**/*.{md,json}": [
      "prettier --write"
    ]
  }
